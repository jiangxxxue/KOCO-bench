{
  "permissions": {
    "allow": [
      "Bash(npm run :*)",
      "Bash(pnpm :*)",
      "Bash(yarn :*)",
      "Bash(git :*)",
      "Bash(supabase :*)",
      "Bash(next :*)",
      "Bash(node :*)",
      "Bash(npx :*)",
      "Bash(tsx :*)",
      "Bash(tsc :*)",
      "Bash(eslint :*)",
      "Bash(prettier :*)",
      "Bash(test :*)",
      "Bash(jest :*)",
      "Bash(vitest :*)",
      "Read(**/*.ts)",
      "Read(**/*.tsx)",
      "Read(**/*.js)",
      "Read(**/*.jsx)",
      "Read(**/*.json)",
      "Read(**/*.md)",
      "Read(**/*.sql)",
      "Read(**/*.css)",
      "Read(**/*.scss)",
      "Read(**/*.env*)",
      "Write(**/*.ts)",
      "Write(**/*.tsx)",
      "Write(**/*.js)",
      "Write(**/*.jsx)",
      "Write(**/*.json)",
      "Write(**/*.md)",
      "Write(**/*.sql)",
      "Write(**/*.css)",
      "Write(**/*.scss)",
      "Edit(**/*.ts)",
      "Edit(**/*.tsx)",
      "Edit(**/*.js)",
      "Edit(**/*.jsx)",
      "Edit(**/*.json)",
      "Edit(**/*.md)",
      "Edit(**/*.sql)",
      "Edit(**/*.css)",
      "Edit(**/*.scss)",
      "MultiEdit(**/*.ts)",
      "MultiEdit(**/*.tsx)",
      "MultiEdit(**/*.js)",
      "MultiEdit(**/*.jsx)"
    ],
    "deny": [
      "Bash(rm -rf :*)",
      "Bash(sudo :*)",
      "Write(**/*.env)",
      "Edit(**/*.env)",
      "Bash(curl:*)",
      "Write(**/node_modules/*)",
      "Edit(**/node_modules/*)"
    ]
  },
  "env": {
    "NODE_ENV": "development",
    "NEXT_TELEMETRY_DISABLED": "1"
  }
}